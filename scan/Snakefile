F1_LVL = 1
F2_LVL = 0
G_START = 4
G_COUNT = 5
BG_START = 4
BG_COUNT = 5

localrules: all

rule all:
    input:
        'none.png'

rule analysis:
    input:
        ai = expand('scans/{f1}_{f2}_{g}_{bg}/', f1=F1_LVL, f2=F2_LVL, g=range(G_START, G_START + G_COUNT), bg=range(BG_START, BG_START + BG_COUNT))
    output:
        'none.png'

    shell:
        '''
        python analysis_ground_state.py {input.ai} {output}
        '''
